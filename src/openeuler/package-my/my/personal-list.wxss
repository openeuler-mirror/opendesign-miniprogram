.table {
  display: flex;
  flex-direction: column;
}

.row {
  display: flex;
}

.header {
  font-weight: bold;
}

.cell {
  flex: 1;
  display: flex;
  text-align: center;
  align-items: center;
  justify-content: center;
  border: 1px solid rgb(75, 73, 73);
  margin: 0 -1px -1px 0;
  padding: 5rpx;
  word-break: break-all;
}
