<view class="container">
    <view class="tabs">
        <view class="tab-item {{index===curIndex?'active':''}}" data-index="{{index}}" bindtap="switchTab"
            wx:for="{{list}}" wx:key="index">{{item.name}}</view>
    </view>
    <view class="gray-bg"></view>
    <view class="msg-box">
        <view class="content">
            <view>
                <image src="../../static/index/null-events.png"></image>
                <text class="title">暂无活动！</text>
                <text>后台正在开发中，敬请期待！</text>
            </view>
        </view>
    </view>

</view>